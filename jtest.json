{
    "testEnvironment": "jest-environment-node",
    "testMatch": ["<rootDir>/lib/*.test.ts", "<rootDir>/schema/*.test.ts"],
    "transform": {
        "^.+\\.ts$": "ts-jest",
        "<transform_regex>": [
            "ts-jest",
            {
                "tsconfig": "tsconfig.json"
            }
        ]
    },
    "verbose": true
}
